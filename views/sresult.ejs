            <small class="text-muted">Available <a href="/sendrequest/<%=data[i].myemail%>"> <button type="button" class="btn btn-outline-success sen-request ">Send Request</button> </a></small>
          </div>

        </div> </a>
      </div>
   
      <%  } %>
